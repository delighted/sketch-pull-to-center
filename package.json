{
  "name": "sketch-pull-to-center",
  "description": "Sketch plugin to move selected shapes or layers toward the center of artboard.",
  "version": "3.0.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-pull-to-center",
    "manifest": "src/manifest.json",
    "main": "sketch-pull-to-center.sketchplugin"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.5"
  },
  "author": "Mike Gowen <mike@delighted.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delighted/sketch-pull-to-center"
  },
  "license": "MIT"
}
